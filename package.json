{
	"name": "cubes",
	"version": "1.0.0",
	"repository": "git@github.com:omarbadran/cubes.git",
	"author": "Omar Badran <engineer.o.badran@gmail.com>",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"clean": "turbo run clean --no-cache --parallel --continue",
		"test": "turbo run test",
		"dev": "turbo run dev",
		"format": "prettier --write '{packages,apps,!build}/**/*.{ts,tsx,js,json,md,yml}'"
	},
	"workspaces": [
		"packages/*",
		"apps/*"
	],
	"devDependencies": {
		"turbo": "^1.4.2"
	},
	"dependencies": {
		"prettier": "^2.7.1"
	}
}
